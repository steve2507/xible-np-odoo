    },
    "odoo": {
      "type": "odoo"
    },
    "model": {
      "type": "odoo.model",
      "maxConnectors": 1
    },
    "identifiers": {
      "type": "math.number"
    }
  },
  "outputs": {
    "done": {
      "type": "trigger"
    }
  }
}